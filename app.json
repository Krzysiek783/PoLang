{
  "expo": {
    "name": "PoLang",
    "slug": "polang",
    "version": "1.0.0",
    "scheme": "polang",
    "platforms": ["android","web"],
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#00C6FF"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
